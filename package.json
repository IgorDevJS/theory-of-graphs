{
  "name": "theory-of-graphs",
  "version": "0.0.1",
  "description": "A project to work with Theory of Graphs",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha test/**/*.spec.js --require babel-register --require ignore-styles",
    "test:tdd": "./node_modules/mocha/bin/mocha test/**/*.spec.js -R nyan --require babel-register --require ignore-styles --watch",
    "test:coverage": "node node_modules/nyc/bin/nyc.js npm test",
    "lint": "node node_modules/eslint/bin/eslint.js src/**/*.js",
    "build:dev": "node node_modules/webpack/bin/webpack.js --mode development",
    "build:prod": "node node_modules/webpack/bin/webpack.js --mode production",
    "serve": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "serve:watch": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js --watch --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IgorDevJS/theory-of-graphs.git"
  },
  "keywords": [
    "theory",
    "graphs",
    "node",
    "js",
    "es6",
    "html",
    "css"
  ],
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "author": "Igor Gon√ßalves",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IgorDevJS/theory-of-graphs/issues"
  },
  "homepage": "https://github.com/IgorDevJS/theory-of-graphs#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.10.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.1.1",
    "node-sass": "^4.9.0",
    "nyc": "^11.7.1",
    "rimraf": "^2.6.2",
    "sass": "^1.3.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "interactjs": "^1.3.3"
  }
}
